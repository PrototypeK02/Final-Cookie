{
  "header": {
    "blocks": [
      "header-layout.desktop",
      "header-layout.mobile"
    ]
  },
  "header.full": {
    "blocks": [
      "header-layout.desktop",
      "header-layout.mobile"
    ]
  },
  "header-layout.desktop": {
    "children": [
      "header-row#4-desktop"
    ]
  },

  "header-row#4-desktop": {
    "children": [
      "logo#desktop",
      "rich-text#cookie"
    ],
    "props": {
      "sticky": true,
      "blockClass": "main-header"
    }
  },

  "autocomplete-result-list.v2": {
    "blocks": ["product-summary.shelf"]
  },
  "search-bar": {
    "blocks": ["autocomplete-result-list.v2"],
    "props": {
      "openAutocompleteOnFocus": true
    }
  },

  "header-row#5-desktop": {
    "children": [
      "vtex.menu@2.x:menu#category-menu",
      "header-spacer",
      "header-spacer",
      "search-bar",
      "login",
      "minicart"
    ],
    "props": {
      "blockClass": "main-topbar"
    }
  },

  "logo#desktop": {
    "props": {
      "title": "Logo",
      "href": "/",
      "url": "https://images-wixmp-ed30a86b8c4ca887773594c2.wixmp.com/f/2644aec9-d617-4cd6-a34f-f47dd8aa2ada/d9ircit-49bc35f9-fb6e-48dc-b120-e030cb38d9c8.png/v1/fill/w_600,h_308,strp/fortune_cookie_comics_logo_by_that_nerdy_art_girl_d9ircit-fullview.png?token=eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzI1NiJ9..WtaCMKyuqe1rwRgdU1lTXBUv_sSQVdq-Jct4z4VdhGk",
      "width": "110"
    }
  },

  "minicart": {
    "blocks": [
      "product-summary"
    ],
    "props": {
      "type": "popup",
      "showRemoveButton": true,
      "showDiscount": true,
      "showSku": true,
      "labelMiniCartEmpty": "",
      "labelButtonFinishShopping": "Go to checkout",
      "enableQuantitySelector": true,
      "maxQuantity": 10,
      "labelClasses": "gray"
    }
  },

  "header-layout.mobile": {
    "children": [
      "header-row#1-mobile"
    ]
  },


  "header-row#1-mobile": {
    "children": [
      "drawer",
      "logo#mobile",
      "header-spacer",
      "header-spacer",
      "minicart"
    ],
    "props": {
      "sticky": true,
      "blockClass": "main-header-mobile"
    }
  },

  "drawer": {
    "children": [
      "menu#drawer"
    ]
  },

  "menu#drawer": {
    "children": [
      "menu-item#category-electronics",
      "menu-item#category-major-appliances",
      "menu-item#category-small-appliances",
      "menu-item#login"
    ],
    "props": {
      "orientation": "vertical"
    }
  },

  "logo#mobile": {
    "props": {
      "title": "Logo",
      "href": "/",
      "url": "https://storecomponents.vteximg.com.br/arquivos/store-theme-logo-mobile.png",
      "width": "70"
    }
  },
  "rich-text#cookie": {
    "props": {
      "blockClass": "cookieHeadingText",
      "textAlignment": "CENTER",
      "textPosition": "RIGHT",
      "text": "Chinese Cookie Fortune",
      "textColor": "c-on-emphasis",
      "font": "t-heading-5"
  }
}
}
